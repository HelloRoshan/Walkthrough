<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="helloDialog"
		title="Hello {/recipient/name}">
		<content>
			<core:Icon
				src="sap-icon://education"
				size="8rem"
				class="sapUiMediumMargin">
			</core:Icon>
		</content>
		<beginButton>
			<Button
				text="{i18n>dialogCloseButton}"
				press= "onCloseDialog"></Button>
		</beginButton>
	</Dialog>
	
</core:FragmentDefinition>
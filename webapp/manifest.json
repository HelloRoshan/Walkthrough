{
	"_version": "1.7.0",
	"sap.app": {
		"id": "sap.ui.demo.walkthrough",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"bookRemote": {
				"uri": "/destinations/northwind/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.walkthrough.view.View1",
			"type": "XML",
			"async": true,
			"id": "view1"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.walkthrough.i18n.i18n"
				}
			},
			"book":{
				"dataSource": "bookRemote"
			}
		},"routing":{
			"config":{
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.demo.walkthrough.view",
				"controlId": "view1",
				"controlAggregation": "pages",
				"async": true
			},
			"routes":[
				{
					"pattern": "",
					"name": "overview",
					"target": "overview"
				},{
					"pattern": "detail/{invoicePath}",
					"name": "detail",
					"target": "detail"
				}
				],
				"targets":{
					"overview":{
						"viewId": "overview",
						"viewName": "Overview"
					},
					"detail": {
						"viewId": "detail",
						"viewName": "Detail"
					}
				}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}